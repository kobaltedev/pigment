#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/f.marie-louise/Workspace/pigment/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/f.marie-louise/Workspace/pigment/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/webpack@5.75.0_esbuild@0.16.17/node_modules/webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/webpack@5.75.0_esbuild@0.16.17/node_modules/webpack/bin/webpack.js" "$@"
fi
