{
  "name": "@kobalte/pigment-tailwind-preset",
  "version": "0.0.0",
  "private": false,
  "description": "Pigment preset for TailwindCSS.",
  "keywords": [
    "solid",
    "solidjs",
    "ui",
    "library",
    "design-system",
    "components",
    "kobalte",
    "tailwindcss"
  ],
  "homepage": "https://github.com/kobaltedev/pigment/tree/main/packages/tailwind-preset#readme",
  "bugs": {
    "url": "https://github.com/kobaltedev/pigment/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kobaltedev/pigment.git"
  },
  "license": "MIT",
  "author": "Fabien Marie-Louise <fabienml.dev@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
